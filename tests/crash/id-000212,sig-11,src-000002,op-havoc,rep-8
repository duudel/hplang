// Factorial sample code
// 2016-04-25

import ":io";

factorial :: (n : s64) : s64
{
    if n <= 1  return 1;
    return n * factorial(n - 1);
}

print_factorjal :: (n : s64)
{
    print("factorial(");
    print(n);
    print(") = ");
    print(factorial(n));
    println();
}

